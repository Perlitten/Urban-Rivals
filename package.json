{
  "name": "urban-rivals-ml-consultant",
  "version": "1.0.0",
  "description": "ML-powered Chrome extension for Urban Rivals game assistance",
  "main": "dist/background/index.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "npm run generate-icons && webpack --mode production",
    "build:stats": "npm run generate-icons && webpack --mode production --profile --json dist/stats.json",
    "analyze": "npm run build:stats && npx webpack-bundle-analyzer dist/stats.json",
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "generate-icons": "node scripts/generate-icons.js"
  },
  "keywords": [
    "urban-rivals",
    "ml",
    "chrome-extension",
    "gaming",
    "ai"
  ],
  "author": "Urban Rivals ML Team",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "dexie": "^3.2.4",
    "immer": "^10.0.3",
    "onnxruntime-web": "^1.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/chrome": "^0.0.251",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "sharp": "^0.34.2",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": [
    "Chrome >= 88"
  ]
}
